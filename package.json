{
  "name": "omnoma-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "APP_VARIANT=development npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "fsd:check": "npx steiger ./src",
    "android:build:prod": "cd android && ./gradlew aR && cd ../",
    "android:install:prod": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "android:uninstall": "adb uninstall com.omnoma.mobile"
  },
  "dependencies": {
    "@effector/reflect": "^9.2.0",
    "@expo/config-plugins": "~9.0.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@withease/i18next": "^23.2.2",
    "effector": "^23.2.3",
    "effector-react": "^23.2.1",
    "effector-storage": "^7.1.0",
    "expo": "^52.0.0",
    "expo-av": "~15.0.1",
    "expo-camera": "~16.0.5",
    "expo-dev-client": "~5.0.2",
    "expo-image-manipulator": "~13.0.5",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.7",
    "i18next": "^23.16.4",
    "intl-pluralrules": "^2.0.1",
    "nativewind": "^4.1.21",
    "patronum": "^2.3.0",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-bootsplash": "^6.2.6",
    "react-native-edge-to-edge": "^1.0.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@feature-sliced/steiger-plugin": "^0.5.0",
    "@react-native-community/cli-tools": "^15.1.2",
    "@types/react": "~18.3.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "patch-package": "^8.0.0",
    "steiger": "^0.5.0",
    "typescript": "~5.3.3"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-splash-screen"
      ]
    }
  },
  "private": true
}
