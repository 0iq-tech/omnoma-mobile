    "effector-react": "^23.2.1",
    "effector-storage": "^7.1.0",
    "expo": "~51.0.28",
    "expo-camera": "~15.0.16",
    "expo-status-bar": "~1.12.1",
    "nativewind": "^4.1.21",
    "patronum": "^2.3.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "3.31.1",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
